{
  "repair": [
    {
      "id": "login_eufy",
      "$template": "login_eufy",
      "options": {
        "logo": "../../../assets/icon.svg",
        "title": {
          "en": "Login with your Eufy account"
        },
        "usernameLabel": {
          "en": "E-mail address",
          "nl": "E-mail adres (Gebruik een extra Eufy admin account)",
          "fr": "Adresse Email - (Utiliser un compte administrateur Eufy supplémentaire)"
        },
        "usernamePlaceholder": {
          "en": "john@doe.com",
          "nl": "john@doe.com",
          "fr": "john@doe.com"
        },
        "passwordLabel": {
          "en": "Password",
          "nl": "Wachtwoord",
          "fr": "Mot de passe"
        },
        "passwordPlaceholder": {
          "en": "Password",
          "nl": "Wachtwoord",
          "fr": "Mot de passe"
        },
        "buttonText": {
          "en": "Login",
          "nl": "Inloggen",
          "fr": "Utilisateur"
        },
        "radioGroup": {
          "en": "Region",
          "nl": "Regio",
          "fr": "Région"
        }
      }
    },
    {
      "id": "loading",
      "template": "loading"
    },
    {
      "id": "login_captcha",
      "$template": "login_captcha",
      "options": {
        "title": {
          "en": "Captcha required",
          "nl": "Captcha vereist",
          "fr": "Captcha nécessaire"
        },
        "captchaLabel": {
          "en": "Captcha",
          "nl": "Captcha",
          "fr": "Captcha"
        },
        "captchaPlaceholder": {
          "en": "A7cF",
          "nl": "A7cF",
          "fr": "A7cF"
        },
        "buttonText": {
          "en": "Login",
          "nl": "Inloggen",
          "fr": "Utilisateur"
        }
      }
    },
    {
      "id": "loading",
      "template": "loading"
    },
    {
      "id": "pincode",
      "template": "pincode",
      "options": {
        "title": {
          "en": "Enter 2FA Code",
          "nl": "Voer 2FA Code in:",
          "fr": "Entrez votre code 2FA"
        },
        "type": "number",
        "length": 6
      },
      "navigation": {
        "prev": "login_eufy"
      }
    },
    {
      "id": "loading",
      "template": "loading"
    },
    {
      "id": "error",
      "$template": "error",
      "navigation": {
        "prev": "login_eufy"
      },
      "options": {
        "title": {
          "en": "Error",
          "nl": "Error",
          "fr": "Erreur"
        },
        "text": {
          "en": "<hr><br><br>To change the login click 'previous'.<hr><br><br>",
          "nl": "<hr><br><br>Om opnieuw in te loggen, klik 'vorige'.<hr><br><br>",
          "fr": "<hr><br><br>Pour changer le login, cliquez sur 'précédent'.<hr><br><br>"
        }
      }
    },
    {
      "id": "done",
      "template": "done"
    },
    {
      "id": "list_devices",
      "template": "list_devices",
      "navigation": {
        "next": "add_devices"
      }
    },
    {
      "id": "add_devices",
      "template": "add_devices"
    },
    {
      "id": "done",
      "template": "done"
    }
  ]
}
